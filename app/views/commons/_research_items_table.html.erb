<div class="table-responsive research-items-table">
  <table class="table table-bordered">
    <thead>
      <% @software_product.research_items_headers.each do |header| %>
        <th><%= header %></th>
      <% end %>
    </thead>
    <tbody>
      <% collection.each do |research_item| %>
        <tr>
          <% research_item.each do |_, value| %>
            <td><%= value %></td>
          <% end %>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
